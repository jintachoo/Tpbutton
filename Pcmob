--made by silas  
local a,b,c=game:getservice("players"),game:getservice("userinputservice"),game:getservice("replicatedstorage")  
local d=a.localplayer  
local e=d:getmouse()  
local f=c:findfirstchild("moveplayer") or c:findfirstchild("requestmovement")  
local g=false  
local h=instance.new("screengui")  
h.name="silasteleportgui"  
h.ignoreguiinset=true  
h.resetonspawn=false  
h.parent=d:waitforchild("playergui")  
local i=instance.new("textbutton")  
i.size=udim2.new(0,140,0,50)  
i.position=udim2.new(1,-150,1,-60)  
i.anchorpoint=vector2.new(0,0)  
i.backgroundcolor3=color3.new(0,0,0)  
i.text=""  
i.font=enum.font.arcade  
i.textscaled=true  
i.parent=h  
i.active=true  
i.draggable=true  
local j=instance.new("textlabel")  
j.size=udim2.new(1,0,0.65,0)  
j.position=udim2.new(0,0,0,0)  
j.backgroundtransparency=1  
j.text="teleport"  
j.textcolor3=color3.new(1,1,1)  
j.font=enum.font.arcade  
j.textscaled=true  
j.parent=i  
--made by silas  
local k=instance.new("textlabel")  
k.size=udim2.new(1,0,0.35,0)  
k.position=udim2.new(0,0,0.65,0)  
k.backgroundtransparency=1  
k.text="made by silas"  
k.textcolor3=color3.fromrgb(150,150,150)  
k.textscaled=true  
k.font=enum.font.gotham  
k.parent=i  
if not b.touchenabled and b.keyboardenabled then  
	h:destroy()  
	local l=instance.new("screengui")  
	l.ignoreguiinset=true  
	l.resetonspawn=false  
	l.parent=d:waitforchild("playergui")  
	local m=instance.new("frame")  
	m.size=udim2.new(1,0,1,0)  
	m.position=udim2.new(0,0,0,0)  
	m.backgroundcolor3=color3.new(0,0,0)  
	m.parent=l  
	local n=instance.new("textlabel")  
	n.size=udim2.new(1,0,1,0)  
	n.position=udim2.new(0,0,0,0)  
	n.backgroundtransparency=1  
	n.text="use m key"  
	n.textcolor3=color3.new(1,1,1)  
	n.font=enum.font.arcade  
	n.textscaled=true  
	n.parent=m  
	task.delay(1,function()  
		l:destroy()  
	end)  
end  
local o=nil  
i.inputbegan:connect(function(p)  
	if p.userinputtype==enum.userinputtype.mousebutton1 or p.userinputtype==enum.userinputtype.touch then  
		o=p.position  
	end  
end)  
i.inputended:connect(function(p)  
	if o and (p.userinputtype==enum.userinputtype.mousebutton1 or p.userinputtype==enum.userinputtype.touch) then  
		local q=p.position  
		if (q-o).magnitude<5 then  
			g=not g  
			j.text=g and "tap to teleport" or "teleport"  
		end  
		o=nil  
	end  
end)  
local function r()  
	return vector3.new(0,2.5+math.random()*2,0)  
end  
local function s(t)  
	local u=d.character  
	if not u then return end  
	local v=u:findfirstchild("humanoidrootpart")  
	if not v then return end  
	local w=t+r()  
	if f then  
		f:fireserver(w)  
	else  
		v.cframe=cframe.new(w)  
	end  
end  
b.inputbegan:connect(function(x,y)  
	if y then return end  
	if x.userinputtype==enum.userinputtype.keyboard and x.keycode==enum.keycode.m then  
		if e.hit then s(e.hit.position) end  
	end  
	if g and (x.userinputtype==enum.userinputtype.mousebutton1 or x.userinputtype==enum.userinputtype.touch) then  
		if e.hit then s(e.hit.position) end  
		g=false  
		j.text="teleport"  
	end  
end)  
--made by silas
